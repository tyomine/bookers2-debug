            <td id="star-rate<%= book.id %>"></td>
            <script>
             $('#star-rate<%= book.id %>').empty();
             $('#star-rate<%= book.id%>').raty({
             size : 36,
             starOff : '<%= asset_path('star/star-off.png') %>',
             starOn : '<%= asset_path('star/star-on.png') %>',
             starHalf: '<%= asset_path('star/star-half.png') %>',
             half : false,
             readOnly: true,
             score: <%= book.rate %>,
             });
            </script>